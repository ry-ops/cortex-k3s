{
  "version": "1.0.0",
  "description": "Natural Language Interface configuration for cortex automation system",
  "intent_definitions": {
    "actions": {
      "CREATE": {
        "keywords": [
          "deploy",
          "create",
          "build",
          "setup",
          "install",
          "provision",
          "start",
          "launch",
          "initialize",
          "spin up",
          "bring up"
