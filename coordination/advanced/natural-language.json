        "requires_confirmation": true,
        "destructive": false
      },
      "READ": {
        "keywords": [
          "show",
          "list",
          "describe",
          "get",
          "check",
          "display",
          "view",
          "inspect",
          "status",
          "what is",
          "tell me about"
