  "task_decomposition_rules": {
    "deploy_k8s_cluster": {
      "preconditions": [
        "infrastructure_credentials_available",
        "talos_config_accessible",
        "sufficient_resources"
      ],
      "tasks": [
        {
          "task_id": "provision_infrastructure",
          "contractor": "infrastructure-contractor",
          "estimated_duration_minutes": 15,
          "estimated_tokens": 5000,
          "subtasks": [
            {
              "name": "create_network",
              "estimated_duration_minutes": 5
            },
            {
              "name": "create_load_balancer",
              "estimated_duration_minutes": 3
            },
            {
              "name": "provision_nodes",
              "estimated_duration_minutes": 8
            }
          ],
          "dependencies": []
        },
        {
          "task_id": "install_kubernetes",
          "contractor": "talos-contractor",
          "estimated_duration_minutes": 15,
          "estimated_tokens": 6000,
          "subtasks": [
            {
              "name": "generate_config",
              "estimated_duration_minutes": 2
            },
            {
              "name": "bootstrap_control_plane",
              "estimated_duration_minutes": 5
            },
            {
              "name": "join_worker_nodes",
              "estimated_duration_minutes": 6
            }
          ],
          "dependencies": ["provision_infrastructure"]
        },
        {
          "task_id": "configure_cluster",
          "contractor": "infrastructure-contractor",
          "estimated_duration_minutes": 12,
          "estimated_tokens": 4000,
          "subtasks": [
            {
              "name": "install_cni",
              "estimated_duration_minutes": 4
            },
            {
              "name": "configure_storage",
              "estimated_duration_minutes": 3
            },
            {
              "name": "setup_ingress",
              "estimated_duration_minutes": 3
            }
          ],
          "dependencies": ["install_kubernetes"]
        },
        {
          "task_id": "verify_deployment",
          "contractor": "monitoring-contractor",
          "estimated_duration_minutes": 5,
          "estimated_tokens": 2000,
          "subtasks": [
            {
              "name": "health_check",
              "estimated_duration_minutes": 2
            },
            {
              "name": "smoke_tests",
              "estimated_duration_minutes": 3
            }
          ],
          "dependencies": ["configure_cluster"]
        }
      ],
      "critical_path": [
        "provision_infrastructure",
        "install_kubernetes",
        "configure_cluster",
        "verify_deployment"
      ],
      "total_estimated_duration_minutes": 47,
      "total_estimated_tokens": 17000
    },
    "deploy_monitoring": {
      "preconditions": [
        "kubernetes_cluster_available",
        "storage_class_configured"
      ],
      "tasks": [
        {
          "task_id": "install_prometheus",
          "contractor": "monitoring-contractor",
