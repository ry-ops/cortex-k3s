          "estimated_tokens": 3000,
          "subtasks": [
            {
              "name": "deploy_prometheus_operator",
              "estimated_duration_minutes": 2
            },
            {
              "name": "configure_service_discovery",
              "estimated_duration_minutes": 2
            },
            {
              "name": "setup_persistence",
              "estimated_duration_minutes": 1
            }
          ],
          "dependencies": []
        },
        {
          "task_id": "install_grafana",
          "contractor": "monitoring-contractor",
          "estimated_duration_minutes": 4,
          "estimated_tokens": 2500,
          "subtasks": [
            {
              "name": "deploy_grafana",
              "estimated_duration_minutes": 2
            },
            {
              "name": "configure_datasources",
              "estimated_duration_minutes": 1
            },
            {
              "name": "import_dashboards",
              "estimated_duration_minutes": 1
            }
          ],
          "dependencies": ["install_prometheus"]
